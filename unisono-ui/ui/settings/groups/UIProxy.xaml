<UserControl x:Class="com.newsarea.search.ui.settings.groups.UIProxy"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Grid>
        <StackPanel x:Uid="StackPanel_1">
            <GroupBox x:Uid="gbGeneral" Name="gbGeneral" Header="HTTP-Proxy" Margin="0,5,0,5">
                <StackPanel x:Uid="StackPanel_2">
                    <TextBox Name="txtHTTPProxy" Height="23" Margin="0,2,0,2" VerticalContentAlignment="Center" />
                </StackPanel>
            </GroupBox>
        </StackPanel>
    </Grid>
</UserControl>
